<div class="row">
  <div class="col-sm-12">
    <form id="user-form" class="form" action="/users/<%= params.id %>?_method=PUT" method="POST">
      <fieldset>
        <legend>Edit you profile</legend>

        <% var errors = (((typeof(user) !== 'undefined') && user.errors) ? user.errors : {}); %>
        <%- partial('form', {user: user, errors: errors}) %>

        <div class="form-group">
          <%- contentTag('input', 'Save', {type: 'submit', class: 'btn btn-primary pull-right'}) %>
        </div>
      </fieldset>
    </form>
  </div>
</div>
