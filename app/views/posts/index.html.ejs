<div class="row">
  <div class="col-sm-8 col-sm-offset-2">
    <div id="posts-list">
      <% if (posts) { %>
        <% for (var i = 0; i < posts.length; i++) { %>
          <%= partial('/posts/show', {post: posts[i], session: session, commentsCollapse: ''}); %>
        <% } %>
      <% } %>
    </div>
  </div>
</div>
